@model IEnumerable<Vidly.Models.Movie>
@{
    ViewBag.Title = "Movies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Movies</h1>
<table class="table table-hover table-bordered">
    <thead>
    <tr>
        <th scope="col">Movies</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var movie in Model)
    {

        <tr>
            <td>
                @movie.Name
            </td>
        </tr>
    }

    </tbody>
</table> 
